{"ast":null,"code":"export default {\n  data() {\n    return {\n      selectedService: '',\n      inputQuantity: '',\n      services: [{\n        id: 1,\n        name: 'Web Design($100)',\n        price: '100'\n      }, {\n        id: 2,\n        name: 'Domain Service($10)',\n        price: '10'\n      }, {\n        id: 3,\n        name: 'SSL($5)',\n        price: '5'\n      }, {\n        id: 4,\n        name: 'Maintenance($50)',\n        price: '50'\n      }],\n      recordStart: 1,\n      records: []\n    };\n  },\n\n  methods: {\n    saveRecord() {\n      let currentService = this.services.find(service => service.id === this.selectedService);\n      let cost = currentService.price * this.inputQuantity;\n      let record = {\n        id: this.recordStart,\n        service: currentService,\n        quantity: this.inputQuantity,\n        cost\n      };\n      this.records.push(record); // console.log(record);\n\n      this.recordStart++;\n    },\n\n    totalRecord() {\n      this.records.cost;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoDA,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,qBAAe,EAAC,EADX;AAELC,mBAAa,EAAC,EAFT;AAGLC,cAAQ,EAAC,CACN;AACEC,UAAE,EAAC,CADL;AAEEC,YAAI,EAAC,kBAFP;AAGEC,aAAK,EAAC;AAHR,OADM,EAMN;AACEF,UAAE,EAAC,CADL;AAEEC,YAAI,EAAC,qBAFP;AAGEC,aAAK,EAAC;AAHR,OANM,EAWN;AACEF,UAAE,EAAC,CADL;AAEEC,YAAI,EAAC,SAFP;AAGEC,aAAK,EAAC;AAHR,OAXM,EAgBN;AACEF,UAAE,EAAC,CADL;AAEEC,YAAI,EAAC,kBAFP;AAGEC,aAAK,EAAC;AAHR,OAhBM,CAHJ;AA0BLC,iBAAU,EAAG,CA1BR;AA2BLC,aAAO,EAAC;AA3BH,KAAP;AA6BD,GA/BY;;AAgCbC,SAAO,EAAE;AACJC,cAAU,GAAE;AACX,UAAIC,cAAa,GAAI,KAAKR,QAAL,CAAcS,IAAd,CAAmBC,OAAO,IAAEA,OAAO,CAACT,EAAR,KAAe,KAAKH,eAAhD,CAArB;AACA,UAAIa,IAAG,GAAIH,cAAc,CAACL,KAAf,GAAuB,KAAKJ,aAAvC;AACA,UAAIa,MAAK,GAAI;AACXX,UAAE,EAAC,KAAKG,WADG;AAEXM,eAAO,EAACF,cAFG;AAGXK,gBAAQ,EAAC,KAAKd,aAHH;AAIXY;AAJW,OAAb;AAMA,WAAKN,OAAL,CAAaS,IAAb,CAAkBF,MAAlB,EATW,CAUX;;AACA,WAAKR,WAAL;AAEA,KAdG;;AAeJW,eAAW,GAAE;AACX,WAAKV,OAAL,CAAaM,IAAb;AACF;;AAjBI;AAhCI,CAAf","names":["data","selectedService","inputQuantity","services","id","name","price","recordStart","records","methods","saveRecord","currentService","find","service","cost","record","quantity","push","totalRecord"],"sourceRoot":"","sources":["/home/kanaung/day_vue_class/src/App.vue"],"sourcesContent":["<template>\n  <div>\n      <div class=\"container\">\n         <div class=\"row\">\n           <div class=\"col-12\">\n             <h3 class=\"text-center text-primary my-3\">Invoice Maker</h3>\n             <form action=\"\" @submit.prevent=\"saveRecord\">\n               <div class=\"row\">\n                 <div class=\"col-6\">\n                   <select class=\"form-select\" v-model=\"selectedService\">\n                     <option value=\"\" disabled>Select Service</option>\n                     <option v-for=\"service in services\" :key=\"service.id\" :value=\"service.id\">\n                       {{service.name}}\n                     </option>\n                   </select>\n                 </div>\n                 <div class=\"col-3\">\n                   <input type=\"number\" v-model=\"inputQuantity\" class=\"form-control\" placeholder=\"Quantity\">\n                 </div>\n                 <div class=\"col-3\">\n                   <button class=\"btn btn-primary\">\n                     <i class=\"fa-solid fa-plus\"></i>\n                   </button>\n                 </div>\n               </div>\n             </form>\n             <table class=\"table table-bordered mt-3\">\n               <thead>\n                   <tr>\n                     <th>#</th>\n                     <th>Service</th>\n                     <th>Quantity</th>\n                     <th>Cost</th>\n                   </tr>\n               </thead>\n               <tbody>\n                   <tr v-for=\"record in records\">\n                      <td>{{record.id}}</td>\n                      <td>{{record.service.name}}</td>\n                      <td>{{record.quantity}}</td>\n                      <td>{{record.cost}}</td>\n                   </tr>\n               </tbody>\n             </table>\n           </div>\n\n         </div>\n      </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      selectedService:'',\n      inputQuantity:'',\n      services:[\n         {\n           id:1,\n           name:'Web Design($100)',\n           price:'100'\n         },\n         {\n           id:2,\n           name:'Domain Service($10)',\n           price:'10'\n         },\n         {\n           id:3,\n           name:'SSL($5)',\n           price:'5'\n         },\n         {\n           id:4,\n           name:'Maintenance($50)',\n           price:'50'\n         }\n\n       ],\n      recordStart :1,\n      records:[],\n    }\n  },\n  methods: {\n       saveRecord(){\n        let currentService = this.services.find(service=>service.id === this.selectedService);\n        let cost = currentService.price * this.inputQuantity;\n        let record = {\n          id:this.recordStart,\n          service:currentService,\n          quantity:this.inputQuantity,\n          cost\n        }\n        this.records.push(record);\n        // console.log(record);\n        this.recordStart++;\n\n       },\n       totalRecord(){\n         this.records.cost\n       }\n  },\n}\n</script>\n\n<style lang=\"scss\">\n@import \"~bootstrap/dist/css/bootstrap.min.css\";\n@import \"~@fortawesome/fontawesome-free/css/all.min.css\";\n\n</style>"]},"metadata":{},"sourceType":"module"}